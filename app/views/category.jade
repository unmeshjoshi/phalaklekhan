extends layout
block content
    div(class="col-md-12 col-md-offset-3")
        div(class="row")
            div
            - each docArray in category
                div(style="margin-top:1%")
                    - each doc in docArray
                        a(href="/phalak/#{doc.name.substring(0, doc.name.indexOf('.'))}", style="margin:0.5%;")
                            img(src="#{doc.url.replace('upload', 'upload/c_limit,h_308,w_228')}", style="width: 12%;")
